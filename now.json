{
  "name": "pageviews",
  "alias": [
    "pageviews.kikobeats.com"
  ],
  "version": 2,
  "regions": [
    "all"
  ],
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "API_KEY": "@pageviews_api_key",
    "DB_URI": "@pageviews_db_uri"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
