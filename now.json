{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DEBUG": "count*"
  },
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
